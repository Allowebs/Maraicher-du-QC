{% extends "./layouts/base.njk" %}

{% block body %}
<p class="lead">Es wurde ein neuer Eintrag angelegt.</p>
<table class="spacer"><tbody><tr><td height="16" style="font-size:16px;line-height:16px;">&nbsp;</td></tr></tbody></table> 

<table class="callout"><tbody><tr><th class="callout-inner secondary">
  <table class="row"><tbody><tr>
    <th class="small-12 large-12 columns first last"><table><tbody><tr><th>
      <p>
        <strong>User:</strong><br>
        {{ user.name }} {{ user.email }}
      </p>
    </th>
<th class="expander"></th></tr></tbody></table></th>
  </tr></tbody></table>
  <table class="row"><tbody><tr>
    <th class="small-12 large-12 columns first last"><table><tbody><tr><th>
      <p>
        <strong>Eintrag:</strong><br>
        <a href="{{ permalink }}" target="_blank" }>{{ entry.name }}</a>
      </p>
    </th>
<th class="expander"></th></tr></tbody></table></th>
  </tr></tbody></table>
</th><th class="expander"></th></tr></tbody></table>

<table class="spacer"><tbody><tr><td height="16" style="font-size:16px;line-height:16px;">&nbsp;</td></tr></tbody></table> 
{% endblock %}


