{
  "name": "@teikei/admin",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.3",
    "date-fns": "2.29.3",
    "feathers-client": "2.4.0",
    "jwt-decode": "3.1.2",
    "prop-types": "15.8.1",
    "ra-data-feathers": "2.9.3",
    "ra-data-json-server": "4.6.2",
    "react": "18.2.0",
    "react-admin": "3.17.2",
    "react-dom": "18.2.0",
    "react-hook-form": "7.41.5",
    "react-scripts": "5.0.1",
    "recharts": "2.2.0",
    "superagent": "8.0.6"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "@babel/preset-react": "7.18.6",
    "@mdx-js/loader": "2.2.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "babel-loader": "9.1.0",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-standard": "17.0.0",
    "eslint-config-standard-jsx": "11.0.0",
    "eslint-config-standard-react": "13.0.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.2.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.31.11",
    "prettier": "2.8.1",
    "pretty-quick": "3.1.3",
    "standard": "17.0.0"
  },
  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true SKIP_PREFLIGHT_CHECK=true FORCE_COLOR=true react-scripts start | cat -",
    "dev": "npm run start",
    "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "clean": "rm -rf build && mkdir build",
    "test": "SKIP_PREFLIGHT_CHECK=true echo 'noop'",
    "eject": "react-scripts eject",
    "lint": "npx eslint . --fix",
    "pretty-quick": "npx pretty-quick --staged",
    "prettier": "npx prettier '**/*.js' --write"
  },
  "eslintConfig": {
    "parser": "@babel/eslint-parser",
    "ignorePatterns": [
      "build/**"
    ],
    "rules": {
      "react/prop-types": 0,
      "react/react-in-jsx-scope": 0,
      "spaced-comment": 0,
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "no-console": 0
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "env": {
      "jest/globals": true,
      "browser": true
    },
    "plugins": [
      "jest"
    ],
    "extends": [
      "standard",
      "standard-jsx",
      "standard-react",
      "plugin:cypress/recommended",
      "prettier"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": false
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
