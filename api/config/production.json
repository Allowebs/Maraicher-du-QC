{
  "host": "localhost",
  "port": "3030",
  "postgres": {
    "client": "pg",
    "connection": "DATABASE_URL"
  },
  "mailer": {
    "nodemailerOptions": {
      "message": {
        "from": "SENDER_EMAIL",
        "replyTo": "REPLY_TO_EMAIL"
      },
      "send": true,
      "preview": false
    },
    "sparkpostTransport": {
      "sparkPostApiKey": "SPARKPOST_API_KEY",
      "endpoint": "SPARKPOST_ENDPOINT"
    },
    "etherealTransport": {
      "host": "smtp.ethereal.email",
      "port": 587,
      "auth": {
        "user": "ETHEREAL_EMAIL",
        "pass": "ETHEREAL_PASSWORD"
      }
    },
    "deliverEmails": "DELIVER_EMAILS",
    "auditRecipients": "AUDIT_RECIPIENTS"
  },
  "errorhandler": {},
  "corsOrigins": "CORS_ORIGINS"
}
