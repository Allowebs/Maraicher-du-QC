{
  "host": "localhost",
  "port": "3030",
  "postgres": {
    "client": "pg",
    "connection": "DATABASE_URL"
  },
  "mailer": {
    "transport": {
      "sparkpost": {
        "sparkPostApiKey": "SPARKPOST_API_KEY",
        "endpoint": "SPARKPOST_ENDPOINT"
      }
    },
    "send": true
  },
  "errorhandler": {},
  "redis": {
    "url": "REDIS_URL"
  },
  "enableJobQueues": true
}
