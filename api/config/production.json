{
  "host": "localhost",
  "port": "3030",
  "postgres": {
    "client": "pg",
    "connection": "DATABASE_URL"
  },
  "mailer": {
    "message": {
      "from": "SENDER_EMAIL",
      "replyTo": "REPLY_TO_EMAIL"
    },
    "transport": {
      "sparkpost": {
        "sparkPostApiKey": "SPARKPOST_API_KEY",
        "endpoint": "SPARKPOST_ENDPOINT"
      }
    },
    "send": true
  },
  "errorhandler": {},
  "enableEmails": "ENABLE_EMAILS",
  "corsOrigins": "CORS_ORIGINS"
}
