{% extends "./layouts/base.njk" %}

{% block body %}
<p class="lead">Es wurde ein neuer Eintrag angelegt.</p>
<table class="spacer"><tbody><tr><td height="16px" style="font-size:16px;line-height:16px;">&#xA0;</td></tr></tbody></table> 

<table class="callout"><tr><th class="callout-inner secondary">
  <table class="row"><tbody><tr>
    <th class="small-12 large-12 columns first last"><table><tr><th>
      <p>
        <strong>User:</strong><br>
        {erb}= "#{@user.name}, #{@user.email}" {/erb}
      </p>
    </th>
<th class="expander"></th></tr></table></th>
  </tr></tbody></table>
  <table class="row"><tbody><tr>
    <th class="small-12 large-12 columns first last"><table><tr><th>
      <p>
        <strong>Eintrag:</strong><br>
        <a href="{erb}= @place.permalink {/erb}" target="_blank" }="">{erb}= @place.name {/erb}</a>
      </p>
    </th>
<th class="expander"></th></tr></table></th>
  </tr></tbody></table>
</th><th class="expander"></th></tr></table>

<table class="spacer"><tbody><tr><td height="16px" style="font-size:16px;line-height:16px;">&#xA0;</td></tr></tbody></table> 
{% endblock %}


